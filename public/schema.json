{
  "openapi": "3.0.0",
  "info": {
    "version": "1.1.0",
    "title": "Manage API",
    "description": "This api is for all endpoints related to the manage area.",
    "contact": {
      "name": "ChilliPharm",
      "url": "https://www.chillipharm.com"
    },
    "license": {
      "name": "UNLICENSED",
      "url": "https://www.chillipharm.com"
    }
  },
  "tags": [
    {
      "name": "Global Form Templates",
      "description": "Endpoints related to global form templates"
    },
    {
      "name": "Form Templates",
      "description": "Endpoints related to form templates"
    }
  ],
  "servers": [
    {
      "url": "http://chillipharm.test:3000/api/v1",
      "description": "Local development"
    },
    {
      "url": "https://qa.chillipharm.dev/api/v1",
      "description": "QA"
    },
    {
      "url": "https://chillipharm-staging.com/api/v1",
      "description": "Staging"
    },
    {
      "url": "https://chillipharm-uat.com/api/v1",
      "description": "UAT"
    },
    {
      "url": "https://www.chillipharm.com/api/v1",
      "description": "Production"
    }
  ],
  "paths": {
    "/manage/form_templates/{id}": {
      "$ref": "refs/paths/manage_form_template_{id}.json"
    },
    "/manage/global_form_templates": {
      "$ref": "refs/paths/manage_global_form_templates.json"
    }
  },
  "components": {
    "securitySchemes": {
      "userSessionKey": {
        "type": "apiKey",
        "in": "cookie",
        "name": "user_session_key",
        "description": "User session key cookie"
      },
      "chillipharmSession": {
        "type": "apiKey",
        "in": "cookie",
        "name": "_chillipharm_session",
        "description": "ChilliPharm session cookie"
      }
    },
    "schemas": {
      "Error": {
        "$ref": "refs/components/schemas/Error.json"
      }
    }
  }
}