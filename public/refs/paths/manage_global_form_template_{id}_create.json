{
  "post": {
    "summary": "Create Form Template from Global Form Template",
    "operationId": "postGlobalFormTemplateCreate",
    "description": "Create a new form template from global form template id.",
    "tags": [
      "Global Form Templates"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "id",
        "in": "path",
        "required": true,
        "description": "The unique identifier of the global form template to be used for creating the form template",
        "schema": {
          "type": "string"
        }
      }
    ],
    "requestBody": {
      "description": "Accoubt ID to create form template",
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "account_id": {
                "type": "string",
                "description": "The unique identifier of the account to be used for creating the form template."
              }
            },
            "additionalProperties": false,
            "required": ["authenticity_token", "account_id"]
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Successful creation response with form template data.",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/FormTemplate.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/Conflict.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  }
}
