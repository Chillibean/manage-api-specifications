{
  "get": {
    "summary": "Get Form Template",
    "operationId": "getManageFormTemplate",
    "tags": [
      "Form Templates"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "description": "Retrieve a specific form template by its ID.",
    "parameters": [
      {
        "name": "id",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string"
        },
        "description": "The unique identifier of the form template."
      }
    ],
    "responses": {
      "200": {
        "description": "Successful response with form template data.",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/FormTemplate.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/Conflict.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  }
}
